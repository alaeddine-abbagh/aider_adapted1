# Company OAuth2 Authentication Configuration
# Copy this file to .env and fill in your actual company credentials

# OAuth2 Client Credentials
OIDC_ENDPOINT=your_oidc_endpoint_here
OIDC_CLIENT_ID=your_client_id_here
OIDC_CLIENT_SECRET=your_client_secret_here
OIDC_SCOPE=your_scope_here

# Azure OpenAI via APIGEE
APIGEE_ENDPOINT=your_apigee_endpoint_here

# Azure OpenAI Configuration
AZURE_AOAI_API_VERSION=2025-04-01-preview
AZURE_AOAI_DEPLOYMENT=gpt5
AZURE_AOAI_DEPLOYMENT_MINI=gpt5mini
AZURE_AOAI_DEPLOYMENT_NANO=gpt5nano

# Optional: Additional configuration
# SSL_VERIFY=true
# TIMEOUT=600
